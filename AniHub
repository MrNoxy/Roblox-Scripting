local Tab = Window:NewTab("Settings")

local Section = Tab:NewSection("Settings")

Section:NewKeybind("ToggleUI", "Toggle UI", Enum.KeyCode.RightShift, function()
	Library:ToggleUI()
end)

Section:NewButton("Discord", "Storm Community discord server link!", function()
	setclipboard("https://discord.gg/k8TBbzbYcc")
	wait(1)
	SolarisLib:Notification("Copied link to clipboard!", "https://discord.gg/k8TBbzbYcc")
end)
