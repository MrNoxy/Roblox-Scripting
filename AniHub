local SolarisLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/sol"))()
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/kav"))()



local Animate = game.Players.LocalPlayer.Character:WaitForChild("Animate")

game.Players.LocalPlayer.CharacterAdded:Connect(function()
	Animate = game.Players.LocalPlayer.Character:WaitForChild("Animate")
end)

SolarisLib:Notification("Loaded!", "AniHub - Made by Noxy#6331")

local Window = Library.CreateLib("AniHub", colors)



local Tab = Window:NewTab("Welcomne")

local Section = Tab:NewSection("Welcome to AniHub!")
local Section = Tab:NewSection("Made by Noxy#6331")
local Section = Tab:NewSection("Helped by $ Slushy#6969")




